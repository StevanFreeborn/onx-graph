{
  "name": "client",
  "version": "1.4.1",
  "private": true,
  "type": "module",
  "release": {
    "repositoryUrl": "https://github.com/StevanFreeborn/onx-graph.git",
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "tagFormat": "v${version}-client"
  },
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host 0.0.0.0",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:stack": "docker compose -f ./docker-compose.test.yml up --build",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "cleanup": "rimraf ./test-results/ ./coverage/ ./dist/ ./tests/coverage/ ./playwright-report/"
  },
  "dependencies": {
    "@microsoft/signalr": "^8.0.0",
    "d3-force": "^3.0.0",
    "jwt-decode": "^4.0.0",
    "pinia": "^2.1.7",
    "rimraf": "^5.0.5",
    "ts-results": "^3.3.0",
    "v-network-graph": "^0.9.15",
    "validator": "^13.11.0",
    "vite-plugin-mkcert": "^1.17.3",
    "vue": "^3.4.19",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.5",
    "@faker-js/faker": "^8.4.1",
    "@pinia/testing": "^0.1.3",
    "@playwright/test": "^1.41.2",
    "@rushstack/eslint-patch": "^1.7.2",
    "@semantic-release/git": "^10.0.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/vue": "^8.0.2",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.11.24",
    "@types/validator": "^13.11.9",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vitest/coverage-istanbul": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "eslint-plugin-playwright": "^1.3.0",
    "eslint-plugin-vue": "^9.22.0",
    "jsdom": "^24.0.0",
    "mailhog": "^4.16.0",
    "mongodb": "^6.5.0",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.2.5",
    "typescript": "^5.4.3",
    "vite": "^5.1.4",
    "vitest": "^1.3.1",
    "vue-tsc": "^1.8.27",
    "zod": "^3.22.4"
  }
}
